{
	"dependencies": {
		"@repo/shared": "workspace:*",
		"hono": "^4.11.9",
		"valibot": "^1.2.0"
	},
	"devDependencies": {
		"@repo/tsup-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@types/node": "^24.0.0",
		"wrangler": "^4.61.1"
	},
	"exports": {
		".": "./dist/index.d.ts"
	},
	"license": "UNLICENSED",
	"name": "@repo/cdn",
	"private": true,
	"scripts": {
		"build": "pnpm run cf-typegen && pnpm run build:tsup && pnpm run build:tsc",
		"build:tsc": "tsc -p tsconfig.build.json",
		"build:tsup": "tsup --clean",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings",
		"deploy": "wrangler deploy --minify",
		"dev": "pnpm run dev:tsup & pnpm run dev:tsc & pnpm run dev:wrangler",
		"dev:tsc": "tsc --watch",
		"dev:tsup": "tsup --watch",
		"dev:wrangler": "wrangler dev",
		"format": "biome check --fix --unsafe .",
		"lint": "biome check .",
		"typecheck": "pnpm run cf-typegen && tsc --noEmit"
	},
	"type": "module",
	"version": "1.0.0"
}
